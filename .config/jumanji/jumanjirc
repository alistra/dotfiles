script 		~/.config/jumanji/scripts/hints.js

searchengine 	ddg 		https://duckduckgo.com/?q=%s
searchengine 	g 		https://encrypted.google.com/search?hl=en&q=%s
searchengine 	wiki		https://secure.wikimedia.org/wikipedia/en/w/index.php?search=%s
searchengine 	maps 		https://maps.google.com/?q=%s
searchengine	torrentz	http://torrentz.com/search?f=%s
searchengine	yt		https://youtube.com/results?search_query=%s

