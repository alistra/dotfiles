urxvt*background: black
urxvt*foreground: white
urxvt*scrollBar: false

urxvt*font: xft:DejaVu Sans Mono:pixelsize=15:antialias=false
urxvt*transparent: yes
urxvt.cursorBlink: false
urxvt.saveLines: 655350
urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: google-chrome
urxvt.matcher.button: 2
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt.termName: rxvt
urxvt.scrollTtyOutput: false
urxvt.scrollWithBuffer: true
urxvt.scrollTtyKeypress: true

xterm*background: black
xterm*foreground: white
xterm*scrollBar: false

xterm*faceName: xft:DejaVu Sans Mono:pixelsize=15:antialias=false
xterm*transparent: yes
xterm.cursorBlink: false
xterm.saveLines: 655350
xterm.perl-ext-common: default,matcher
xterm.urlLauncher: google-chrome
xterm.matcher.button: 2
xterm.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
xterm.scrollTtyOutput: false
xterm.scrollWithBuffer: true
xterm.scrollTtyKeypress: true
XTerm*metaSendsEscape: true
