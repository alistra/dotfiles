#!/bin/zsh
#{{{bialobrewy
if [ "`hostname`" = "adeli" ]
then
	if [ "$ZSHINIT" = "dev" ]
	then
		export PYTHONPATH="/var/django"
		cd /var/django/archfinch
	fi #}}}
#{{{adeli
elif [ "`hostname`" = "bialobrewy" ] 
then
	if [ "$ZSHINIT" = "rails" ]
	then
		Imgur-Directory-Listing/script/server
	elif [ "$ZSHINIT" = "seriale" ]
	then
		cd /torrents
	elif [ "$ZSHINIT" = "current" ]
	then
		cd /torrents
		fresh
	fi
fi #}}} 
#{{{all
if [ "$ZSHINIT" = "mail" ]
then
	alpine
elif [ "$ZSHINIT" = "vim" ]
then
	vim $VIMFILE
	unset VIMFILE
elif [ "$ZSHINIT" = "irc" ]
then
	ssh -t spock 'ZSHINIT=irc zsh'
elif [ "$ZSHINIT" = "wget" ]
then
	wget -nc --adjust-extension --load-cookies ~/.config/jumanji/cookies -O "$WGETOUTFILE" -- "$WGETFILE"
	local TMP
	TMP=$WGETOUTFILE
	unset WGETFILE, WGETOUTFILE
	exec xdg-open $TMP &
	disown
	unset ZSHINIT
	exit
fi #}}}
unset ZSHINIT
